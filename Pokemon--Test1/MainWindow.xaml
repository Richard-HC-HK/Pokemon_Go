                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock x:Name="HPBlock3" Grid.Column="2" HorizontalAlignment="Left" Height="16" Margin="146,88,0,0" Grid.Row="1" TextWrapping="Wrap" Text="HP" VerticalAlignment="Top" Width="30"/>
